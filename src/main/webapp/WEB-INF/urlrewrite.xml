<?xml version="1.0"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
        "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>  
  <rule>
    <from>/images/**</from>
    <to>/images/$1</to>
  </rule>
  <rule>
    <from>/scripts/**</from>
    <to>/scripts/$1</to>
  </rule>
  <rule>
    <from>/styles/**</from>
    <to>/styles/$1</to>
  </rule>
  <rule>
    <from>/**</from>
    <to>/app/$1</to>
  </rule>
</urlrewrite>